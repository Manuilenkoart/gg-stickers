{
  "name": "gg-stickers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev & tsc --noEmit --watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write",
    "type-check": "tsc --project tsconfig.json --pretty --noEmit",
